{
  "name": "reduxdb",
  "version": "1.0.0",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc -d --lib 'es6' --outFile index.js js/index.ts",
    "watch": "tsc -d --lib 'es6' --outFile index.js -w js/index.ts",
    "test": "tsc test/test_*.ts && ls test/test_*.js |xargs -I {} node {}",
    "prepublish": "npm run build && npm run test"
  },
  "repository": "git@github.com:wizawu/reduxdb.git",
  "devDependencies": {
    "typescript": "2.0.x"
  },
  "dependencies": {
    "object-assign": "^4.0.1",
    "object-path": "^0.9.2"
  },
  "peerDependencies": {
    "redux": "^3.3.1"
  }
}
